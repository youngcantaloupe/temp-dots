# Aliases
alias cat=batcat
alias vim=nvim
alias ls="eza --color=always --long --git --icons=always --sort=type"
alias ls-h="cat ~/.config/zsh/eza.help.md"
alias harpoon-h="cat ~/.config/nvim/lua/plugins/harpoon_help.md"
alias animebg="feh --randomize --bg-fill ~/Documents/Pictures/AnimeWallpapers/Current"
alias dunebg="feh --bg-fill ~/Documents/Pictures/Wallpapers/Dune.png"
alias blankbg="feh --bg-fill ~/Documents/Pictures/Wallpapers/BlankBackground.jpg"
alias greenbg="feh --bg-fill ~/Documents/Pictures/Wallpapers/GreenBlankBackground.png"
alias neofetcha="neofetch --ascii ~/.local/bin/pikachu.txt --ascii_colors 2"
